{
  "private": true,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "tailwindcss --output ./app/styles/app.css --minify --postcss",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "tailwindcss --output ./app/styles/app.css --watch --postcss",
    "dev:remix": "remix dev",
    "lint": "eslint --cache --ignore-path .gitignore --fix .",
    "format": "prettier --ignore-path .gitignore --write .",
    "typecheck": "tsc",
    "validate": "run-p lint format typecheck"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@paralleldrive/cuid2": "^2.0.1",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@remix-run/vercel": "^1.12.0",
    "@vercel/node": "^2.9.1",
    "copy-lite": "^0.1.2",
    "hyperid": "^3.1.1",
    "nanoid": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^6.0.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.11.19",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-100vh-fix": "^1.0.2",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
